{
    "name": "switchyd",
    "version": "0.7.4.0",
    "manifest_version": 2,
    "incognito":"spanning",
    
    "background":{
        "persistent": true,
        "scripts": ["proxy.js"]
    },

    "options_page": "option.html",

    "permissions": [
        "proxy",
        "webRequest",
        "http://*/*",
        "https://*/*"
    ],

    "content_security_policy": "object-src 'self';script-src 'self' 'unsafe-eval';style-src 'self' 'unsafe-inline';"
}
