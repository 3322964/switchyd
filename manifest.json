{
	"name": "switchyd",
	"version": "0.5.2",
	"manifest_version": 2,
    
	"background":{
        "persistent": true,
        "scripts": ["proxy.js"]
    },
    
    "options_page": "options.html",

	"permissions": [
        "alarms",
        "storage",
        "proxy",
        "webRequest",
        "http://*/*",
        "https://*/*"
	]
}