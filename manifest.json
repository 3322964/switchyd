{
	"name": "switchyd",
	"version": "0.2.14",
	"manifest_version": 2,
    
	"background":{
        "persistent": true,
        "scripts": ["proxy.js"]
    },

	"permissions": [
        "alarms",
        "storage",
        "proxy",
        "webRequest",
        "http://*/*",
        "https://*/*"
	]
}