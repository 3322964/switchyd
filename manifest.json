{
    "name": "switchyd",
    "version": "0.7.4.0",
    "manifest_version": 2,
    "incognito":"spanning",
    
    "background":{
        "persistent": true,
        "scripts": ["proxy.js"]
    },

    "options_page": "polymer_option.html",

    "permissions": [
        "proxy",
        "webRequest",
        "http://*/*",
        "https://*/*"
    ]
}
