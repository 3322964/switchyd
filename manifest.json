{
	"name": "Proxy SwitchyD!",
	"version": "0.1",
	"manifest_version": 2,
	
	"default_locale": "en",
    
	"background":{
        "persistent": true,
        "scripts": ["proxy.js"]
    },

	"permissions": [
        "storage",
        "proxy",
        "webRequest",
        "http://*/*",
        "https://*/*"
	]
}